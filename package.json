{
  "name": "electron-app",
  "version": "1.0.0",
  "description": "a electron app demo",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "postinstall": "electron-builder install-app-deps",
